<!DOCTYPE html>
<html>
  <head>
    <title>
      <%= title %>
    </title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1>
       <%= title %>
      </h1>
    <p>Welcome to <%= title %></p>
    <table>
      <tr>
        <th width ="100">id</th>
        <th width ="100">title</th>
        <th width ="100">description</th>
        <th width ="100">reference</th>
      </tr>
      <% items .forEach((item)=> { %>
        <tr>
          <td>
            <%= item.id %>
          </td>
          <td>
            <%= item.title %>
          </td>
          <td>
              <%= item.description %>
            
            </td>
            <td>
              <%= item.reference %>

            </td>
        </tr>

     <% }); %>
    </table>

  </body>
</html>
